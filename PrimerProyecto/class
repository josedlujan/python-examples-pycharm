#print(d,e + 2)

#variables en cadenas
#las cadenas por defecto son Unicode en Python 3 y texto ASCII en python 2
a = 1
b = 2
print(' el num a es {} y el num b es {}'.format(1,b))

c1 = 'aa'
c2 = "aa"
c3 = """aa"""

print(c1)
print(c2)
print(c3)

#flotantes
m = 2.9
print(m)

